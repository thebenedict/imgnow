{
  "manifest_version": 2,

  "name": "imgnow",
  "description": "Get short image urls, now be witty",
  "version": "1.0",

  "permissions": [
    "https://www.googleapis.com/*",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon.png",
             "48": "icon_48.png" }
}